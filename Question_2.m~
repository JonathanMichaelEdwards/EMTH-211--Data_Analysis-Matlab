clear, clc;

% Loading in matrix X
load('A1.mat')

% Extracting x and 
x = X(1, :)';
y = X(2, :)';
A = [x.^2 y.^2 x.*y x y];
b = ones(length(X), 1);
c = A\b;

% Initialising F as -1
F = -1;

f = @(u, v) c(1)*u.^2 + c(2)*v.^2 + c(3)*u.*v + c(4)*u + c(5)*v + F;

figure(1);
scatter(x, y);
hold on
fimplicit(f);
hold off
grid
title('Least Square Approximation for Conic Section in a Plane');
xlabel('x')
ylabel('y')
legend('Test Data', 'Approximation');
ylim([-0.8 0.5]) 